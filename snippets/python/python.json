{
    "Optimal type checking imports through if TYPE_CHECKING": {
        "prefix": "typeim",
        "body": [
            "from __future__ import annotations",
            "from typing import TYPE_CHECKING",
            "if TYPE_CHECKING:  # pragma: no cover",
            "  from ${1:module} import ${2:dependency}"
        ],
        "description": "Optimal type checking imports through if TYPE_CHECKING"
    }
}
